# Behat/REPL Test Configuration (Local Development)
# Timeout for reading REPL output in tests (seconds)
REPL_OUTPUT_TIMEOUT=1.5

# Timeout for waiting for prompt (seconds)
REPL_PROMPT_TIMEOUT=0.15

# Debug logging (true = always log, false = only log on failure)
REPL_DEBUG=false

# Test mode (true = use sentinel markers, false = use prompt detection)
REPL_TEST_MODE=true
